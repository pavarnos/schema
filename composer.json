    "type": "library",
    "version": "0.1",
    "license": "MIT",
    "authors": [
        {
        "name": "Steve Pavarno",
        "email": "steve@lightlysalted.co.nz",
        "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "LSS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LSS\\": "test"
        }
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    }
}
